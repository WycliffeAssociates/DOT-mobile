{
  "name": "dotmobile",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production npm run lint && tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "playwright test",
    "test.unit": "vitest",
    "lint": "biome check --apply ./src/**/*",
    "ios": "npm run build && npm run sync && npx cap run ios",
    "android": "npm run build && npx cap run android",
    "sync": "npx cap sync",
    "iconGenerate": "npx @capacitor/assets generate --iconBackgroundColor '#eeeeee' --splashBackgroundColor '#eeeeee'",
    "updateAllPackages": "npm outdated | awk 'NR>1 {print $1\"@\"$4}' | xargs npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "^5.5.1",
    "@capacitor/device": "^5.0.6",
    "@capacitor/filesystem": "^5.1.1",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/ios": "^5.5.1",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/share": "^5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@ionic/react": "^7.5.7",
    "@ionic/react-router": "^7.5.7",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/video.js": "^7.3.56",
    "capacitor-blob-writer": "^1.1.14",
    "i18next": "^23.7.7",
    "i18next-browser-languagedetector": "^7.2.0",
    "ionicons": "^7.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@capacitor/assets": "^3.0.1",
    "@capacitor/cli": "^5.5.1",
    "@playwright/test": "^1.40.1",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@unocss/transformer-variant-group": "^0.57.7",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "jsdom": "^23.0.1",
    "terser": "^5.4.0",
    "typescript": "^5.3.2",
    "unocss": "^0.57.7",
    "vite": "^5.0.4",
    "vitest": "^0.34.6"
  },
  "description": "WA Sign language bibles app"
}
